---
layout: null
permalink: /comic/feed.xml
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>comic - {{ site.title }}</title>
 <link href="{{ site.url }}{{ site.baseurl }}/comic/feed.xml" rel="self"/>
 <link href="{{ site.url }}{{ site.baseurl }}/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>{{ site.url }}</id>
 <author>
   <name>{{ site.author.name }}</name>
 </author>

 {% for post in site.categories.comic limit:10 %}
 {% if post.layout == "comic" %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="{{ site.url }}{{ site.baseurl }}{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>{{ site.url }}{{ post.id }}</id>
   <content type="html">
     &lt;img src=&quot;{{ site.baseurl }}/assets/comic/{{ post.comic_id }}.png&quot;&gt;
     {{ post.content | xml_escape }}
   </content>
 </entry>
 {% endif %}
 {% endfor %}

</feed>
